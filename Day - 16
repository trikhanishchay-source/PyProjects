from prettytable import PrettyTable
table = PrettyTable()
''' you need to add the package pretty table to use it.'''
table.add_column("Pokemon Name",["Pikachu", "Squirtle" , "Charmender"])
table.add_column("Type", ["Electric","Water","Fire"])
table.align = "l"

print(table)
